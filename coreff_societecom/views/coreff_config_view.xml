<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_coreff_societecom_config" model="ir.ui.view">
            <field name="name">Coreff: Configuration</field>
            <field name="model">coreff.config.settings</field>
	      	<field name="inherit_id" ref="coreff_base.view_coreff_config"/>
            <field name="arch" type="xml">
                
                	<xpath expr="//header" position="after">   
                    <group string="Informations Societe.com" name="coreff">
                        <field name="societeComUrl"/>
                        <field name="societeComLogin"/>
                        <field name="societeComPassword"/>
                    </group>
                </xpath>
                
            </field>
        </record>

    </data>
</odoo>