<odoo>
    <record id="ir_cron_creditsafe_authent" 
        model="ir.cron">
        <field name="name">CreditSafe : Authent</field>
        <field name="model_id" 
            ref="model_creditsafe_api" />
        <field name="state">code</field>
        <field name="code">model.authenticate()</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" 
            name="doall" />
    </record>
</odoo>